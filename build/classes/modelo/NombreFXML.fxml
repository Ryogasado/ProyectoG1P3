<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="modelo.ConfiguracionFXMLController">
   <children>
      <AnchorPane layoutX="101.0" layoutY="36.0" prefHeight="126.0" prefWidth="399.0">
         <children>
            <ChoiceBox fx:id="oponentes" layoutX="176.0" layoutY="68.0" onMouseClicked="#clickOponentes" prefWidth="150.0" />
            <Label fx:id="escogerOp" layoutX="14.0" layoutY="36.0" prefHeight="27.0" prefWidth="269.0" text="Escoja el numero de oponentes:" textAlignment="CENTER">
               <font>
                  <Font name="Comic Sans MS" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="105.0" layoutY="162.0" prefHeight="165.0" prefWidth="390.0">
         <children>
            <Label fx:id="visibCartas" layoutX="22.0" layoutY="40.0" text="Visibilidad de Cartas:">
               <font>
                  <Font name="Comic Sans MS" size="18.0" />
               </font>
            </Label>
            <ChoiceBox fx:id="visibilidad" layoutX="179.0" layoutY="75.0" onMouseClicked="#clickVisibilidad" prefWidth="150.0" />
         </children>
      </AnchorPane>
      <Button fx:id="btnAceptar" layoutX="275.0" layoutY="327.0" mnemonicParsing="false" onAction="#accionAceptar" text="Aceptar" />
      <Button fx:id="regresar" layoutX="562.0" layoutY="14.0" mnemonicParsing="false" onAction="#accionRegresar" text="X" />
   </children>
</AnchorPane>
